    "def get_desc_taxa(node, cand_taxa):\n",
    "    desc_taxa = []\n",
    "    if node.sci_name in cand_taxa:\n",
    "        desc_taxa.append(node.sci_name)\n",
    "        return desc_taxa\n",
    "    for ch in node.children:\n",
    "        desc_taxa.extend(get_desc_taxa(ch, cand_taxa))\n",
    "    return desc_taxa"
   ]
  }
